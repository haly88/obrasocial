<table>
  <thead>
    <tr>
      <th>Tipo</th>
      <th>Fecha</th>
      <th>Nombre</th>
      <th>Cantidad</th>
      <th>Precio</th>
      <th>Km</th>
      <th>Total</th>
    </tr>
  </thead>

  <tbody>
    <% @prestaciones.each do |c| %>
      <tr>
        <td>
          <% if c.domiciliaria == true %>
            Domiciliaria
          <% else %>
            Basica
          <% end %>
        </td>
        <td><%= c.fecha %></td>
        <td><%= c.nombre %></td>
        <td><%= c.cantidad %></td>
        <td><%= c.precio %></td>
        <td><%= c.km %></td>
        <td><%= c.total %></td>
      </tr>
    <% end %>
  </tbody>
</table>