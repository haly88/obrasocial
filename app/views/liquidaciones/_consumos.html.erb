<table>
  <thead>
    <tr>
      <th>Insumo</th>
      <th>Cantidad</th>
      <th>precio</th>
      <th>total</th>
    </tr>
  </thead>

  <tbody>
    <% @consumos.each do |c| %>
      <tr>
        <td><%= c.insumo %></td>
        <td><%= c.cantidad %></td>
        <td><%= c.precio %></td>
        <td><%= c.total %></td>
      </tr>
    <% end %>
  </tbody>
</table>